<Window x:Class="EJournalAutomate.Views.Windows.AutoDownloadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EJournalAutomate.Views.Windows"
        mc:Ignorable="d"
        Title="Авто загрузка" WindowStartupLocation="CenterOwner"
        Height="180" Width="330">
    <StackPanel>
        <StackPanel Orientation="Horizontal" Margin="10">
            <TextBlock Text="Группа:" FontSize="14" Width="100"/>
            <ComboBox Width="200"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Margin="10">
            <TextBlock Text="Предмет:" FontSize="14" Width="100"/>
            <ComboBox Width="200"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Margin="10">
            <TextBlock Text="Практическая:" FontSize="14" Width="100"/>
            <ComboBox Width="200"/>
        </StackPanel>
        <Button Content="Скачать" Width="70" Margin="0,10,0,0" FontSize="14"/>
    </StackPanel>
</Window>
